    "Metadata": {
        "Id": "97e834f4-9a45-450c-8f38-f6261cc1d8e2",
        "Title": "Through the Lion&apos;s Den",
        "ScenePath": "assembly:/_pro/scenes/missions/theark/scene_magpie.entity",
