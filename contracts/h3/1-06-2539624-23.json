    "Metadata": {
        "Id": "d23fe612-0b49-4a83-a6ca-21f500cbe115",
        "Type": "usercreated",
        "Title": "Seven Years All Different Accidents",
        "Location": "LOCATION_MARRAKECH",
        "PublicId": "106253962423",
        "ScenePath": "assembly:/_pro/scenes/missions/marrakesh/_scene_spider.entity",
