    "Metadata": {
        "Id": "7fe67e09-1156-4154-8f76-0393a5633d18",
        "Title": "The Ghost of the Amalfi Coast",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/CoastalTown/_scene_octopus.entity",
