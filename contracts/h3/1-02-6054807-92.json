    "Metadata": {
        "Id": "70109219-5e4f-426d-9d61-d61e5564c7dd",
        "Title": "Bread, more like dead",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Paris/_scene_paris.entity",
