    "Metadata": {
        "Id": "d1bb60c8-bab3-4172-8ff7-2a19b93b2976",
        "Type": "usercreated",
        "Title": "Seven Years of Easy Luck",
        "Location": "LOCATION_MARRAKECH",
        "PublicId": "106270514516",
        "ScenePath": "assembly:/_pro/scenes/missions/marrakesh/_scene_spider.entity",
