    "Metadata": {
        "Id": "cb7fe316-cc5b-48c0-be4a-7909ddfb75bf",
        "Type": "usercreated",
        "Title": "A Song of Knife and Fire",
        "Location": "LOCATION_HOKKAIDO",
        "PublicId": "110150117783",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Hokkaido/_scene_snowcrane.entity",
        "TileImage": "$($repository 2e1a30c5-b1e0-4984-b3f4-67f01f9ebfef).Image",
        "Description": "Something shady is going on in the basement of Hokkaido&apos;s GAMA Hospital. We&apos;ve heard rumours of human experimentation. We want you to investigate, but do so quietly. Appropriate attire for a stealth recon mission will be provided. Your target are the hospital director, the test subject  and the two main researchers, Tenri Shinosaki and Katashi Ito. Akira Nakamura should be given an appropriate execution. As for the rest, make it look like an accident. But make it hurt. Good luck, 47.",
        "GameVersion": "8.18.0.0",
        "IsPublished": true,
        "Entitlements": [
            "H1_LEGACY_STANDARD"
        ],
        "CreatorUserId": "29083990-5489-4cfc-bbc5-4b462bd86e49",
        "ServerVersion": "8.18.0.68",
        "CreationTimestamp": "2025-01-09T04:07:46.4085981Z",
