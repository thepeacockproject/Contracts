    "Metadata": {
        "Id": "7525a922-6ff3-49db-b90b-555711517065",
        "Type": "usercreated",
        "Title": "Demise Of The Eternal Love",
        "Location": "LOCATION_HOKKAIDO",
        "PublicId": "110060974544",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Hokkaido/_scene_snowcrane.entity",
