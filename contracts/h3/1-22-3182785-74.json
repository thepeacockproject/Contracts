    "Metadata": {
        "Id": "74f3e0da-c85c-4f6d-afca-caf3f038dc2d",
        "Type": "usercreated",
        "Title": "Mayhem in Whittleton",
        "Location": "LOCATION_NORTHAMERICA",
        "PublicId": "122318278574",
        "ScenePath": "assembly:/_pro/scenes/missions/skunk/scene_skunk.entity",
