    "Metadata": {
        "Id": "29e25495-d40d-487c-befb-c6a33aca86cf",
        "Type": "usercreated",
        "Title": "The Turning Tide",
        "Location": "LOCATION_COASTALTOWN_EBOLA",
        "PublicId": "115795027570",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/CoastalTown/scene_ebola.entity",
        "TileImage": "$($repository 1d37c552-2e9d-4bb0-8782-4198a7354e40).Image",
        "Description": "Good evening, 47.\r\rFive individuals have been frequenting the pier after dark, dismissing local superstition as nothing more than folklore.\r\rLet their fates reinforce the superstition they dismissed, their deaths appearing entirely accidental, as though the sea had already laid claim to them.\r\rBy dawn, the only disturbance left should be the tide.",
        "GameVersion": "8.22.0.0",
        "IsPublished": true,
