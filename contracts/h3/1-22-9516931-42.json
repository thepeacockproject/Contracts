    "Metadata": {
        "Id": "c90483e9-0eb7-4e42-8bd6-61374a335f23",
        "Type": "usercreated",
        "Title": "The Muffin Man",
        "Location": "LOCATION_NORTHAMERICA",
        "PublicId": "122951693142",
        "ScenePath": "assembly:/_pro/scenes/missions/skunk/scene_skunk.entity",
