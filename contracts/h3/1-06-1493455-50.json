    "Metadata": {
        "Id": "09671cd7-c038-4678-a6fa-36684d8becff",
        "Type": "usercreated",
        "Title": "Military Destabilization",
        "Location": "LOCATION_MARRAKECH",
        "PublicId": "106149345550",
        "ScenePath": "assembly:/_pro/scenes/missions/marrakesh/_scene_spider.entity",
