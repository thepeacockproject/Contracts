    "Metadata": {
        "Id": "d5531fa3-a882-4672-bd12-2570169b559c",
        "Type": "usercreated",
        "Title": "Оптимальный способ с предсказателем",
        "Location": "LOCATION_MARRAKECH",
        "PublicId": "106651258184",
        "ScenePath": "assembly:/_pro/scenes/missions/marrakesh/_scene_spider.entity",
