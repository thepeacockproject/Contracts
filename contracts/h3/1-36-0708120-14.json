    "Metadata": {
        "Id": "ae7ff538-8cb4-46c5-858b-9d412dfef847",
        "Type": "usercreated",
        "Title": "Merry Massacre",
        "Location": "LOCATION_PARIS_NOEL",
        "PublicId": "136070812014",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Paris/_scene_paris.entity",
