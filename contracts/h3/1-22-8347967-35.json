    "Metadata": {
        "Id": "670c0b1b-94fd-4844-b439-1aedfbf223b4",
        "Title": "Do you know Die Muffin-Leute?",
        "ScenePath": "assembly:/_pro/scenes/missions/skunk/scene_skunk.entity",
