    "Metadata": {
        "Id": "cedc3587-9c94-48e7-8271-236f59214893",
        "Title": "Bad Hiring",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Mumbai/scene_mongoose.entity",
        "Location": "LOCATION_MUMBAI",
        "Description": "A production company, currently making a film in Rangan Tower,  has payed us to eliminate four of the films actors and one of their crew members.\r\rAccording to the client the crew member hired too many of the same role, leading the company to not want to pay the extra actors.  By eliminating them they remove any concerns of the actors taking the company to court later on.\r\rThe client has asked that all kills should be accidents to remove any suspicion.\r\rGood luck 47.",
        "CreatorUserId": "b9515510-7e53-48d5-8bcb-b38b40e30df2",
        "GameVersion": "8.6.0.0",
        "ServerVersion": "8.6.0.30",
        "Type": "usercreated",
