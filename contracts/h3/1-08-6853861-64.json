    "Metadata": {
        "Id": "557eb942-971e-452b-a059-e0fb85b4a351",
        "Type": "usercreated",
        "Title": "Winning in court",
        "Location": "LOCATION_BANGKOK",
        "PublicId": "108685386164",
        "ScenePath": "assembly:/_PRO/Scenes/Missions/Bangkok/_scene_tiger.entity",
